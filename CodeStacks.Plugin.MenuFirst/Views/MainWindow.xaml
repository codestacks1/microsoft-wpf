<Window x:Class="codectacks.plugin.menufirst.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:codectacks.plugin.menufirst.Views"
        xmlns:viewmodel="clr-namespace:codectacks.plugin.menufirst.ViewModels"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:gmap="http://xiaowen.codestacks.wpf.gmap"
        mc:Ignorable="d"
        Height="768" Width="1366"
        d:DataContext="{d:DesignInstance viewmodel:MainWindowViewModel,IsDesignTimeCreatable=True}"
        prism:ViewModelLocator.AutoWireViewModel="True" Icon="/CodeStacks.Plugin.MenuFirst;component/logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <gmap:MyMapControl />
    </Grid>
</Window>
